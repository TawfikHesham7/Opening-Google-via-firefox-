package samplePackagee;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class FireFoxDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setProperty("webdriver.gecko.driver", "Value") ; // put the value as agter putting the key 

		WebDriver driver = new FirefoxDriver() ; 
		driver.navigate().to("http://www.google.com"); // Opening google via firefox 
		driver.close() ; //Closing Firefox 
	}

}
