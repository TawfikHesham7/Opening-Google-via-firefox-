package samplePackagee;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class FireFoxDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setProperty("webdriver.gecko.driver", "C:\\Users\\engh1\\OneDrive\\Desktop\\Selenium Course\\Configues\\firefox\\geckodriver.exe") ; 

		WebDriver driver = new FirefoxDriver() ; 
		driver.navigate().to("http://www.google.com"); // Opening google via firefox 
		driver.close() ; //Closing Firefox 
	}

}
